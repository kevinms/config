{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    },
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gdscript", "gd"],
      "port": 6008
    }
  }
}
